# Firebase Configuration
# Copy this file to .env and fill in your Firebase project details
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# LiveKit Configuration
# Get these from your LiveKit Cloud or self-hosted server
VITE_LIVEKIT_URL=wss://your-livekit-server.com
VITE_LIVEKIT_API_KEY=your-api-key
VITE_LIVEKIT_API_SECRET=your-api-secret

# Slack Integration (optional)
# Feature flag to enable/disable Slack UI and notifications.
# Accepts: true/false/1/0 (case-insensitive). Default is true when unset.
VITE_SLACK_FEATURE_ENABLED=true
# Cloud Function proxy endpoint that posts to Slack (do not put tokens in frontend env!)
VITE_SLACK_FUNCTION_URL=https://your-cloud-function-url
# Target channel ID to post messages (e.g., C0123456789)
VITE_SLACK_CHANNEL_ID=
